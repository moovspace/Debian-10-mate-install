{
	"name": "php-import-checker",
	"displayName": "PHP import checker",
	"description": "Know when you are importing a class and is not using it in the file, easy and fast, inspired by PHPStorm",
	"version": "0.0.3",
	"publisher": "grifinas",
	"repository": "git@github.com:grifinas/php-import-checker.git",
	"icon": "arrows.png",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sayHello",
		"onLanguage:php"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "Check PHP import"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.48",
		"@types/node": "^6.0.111",
		"mocha": "^2.3.3",
		"typescript": "^2.8.3",
		"vscode": "^1.1.18"
	},
	"__metadata": {
		"id": "e1ddfeda-17fb-4d05-8031-ee5fef43e51e",
		"publisherId": "e80c549c-2704-4ab3-ae06-efb1066a4489",
		"publisherDisplayName": "grifinas"
	}
}